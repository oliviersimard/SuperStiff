#comments with '#'
model          plaquetteC2vSc.model
ITERSTART      0
ITEREND        100

#USE_UPDATES
START_FROM_HYB
#START_FROM_SELF

parameters
t       1.0
tp      -0.1
#tpp     0.20
U       8.0m
*

